<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="88.0" prefWidth="301.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
   <children>
      <Label layoutX="117.0" layoutY="2.0" text="Welcome">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Button layoutX="248.0" layoutY="51.0" mnemonicParsing="false" onAction="#quitPressed" text="Quit" />
      <Button fx:id="loginButton" layoutX="184.0" layoutY="51.0" mnemonicParsing="false" onAction="#loginPressed" text="Login" />
      <TextField fx:id="usernameField" layoutX="14.0" layoutY="51.0" promptText="username" />
   </children>
</AnchorPane>
